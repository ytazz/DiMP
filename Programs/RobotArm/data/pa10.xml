<?xml version="1.0" encoding="utf-8"?>

<namespace name="pa10">
  <attr unit_length="m"/>
  <attr unit_mass  ="kg"/>
  
  <attr mass="1.0" inertia="1.0"/>
  <gravity accel="0 0 0"/>

  <vmat name="mat_body"  colorname="darkturquoise"/>
  <vmat name="mat_cover" colorname="white"/>
    
  <mesh name="mesh_base"      scale="0.01" filename="base_binary.stl"      visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link1"     scale="0.01" filename="link1_binary.stl"     visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link1_cap" scale="0.01" filename="link1_cap_binary.stl" visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link2a"    scale="0.01" filename="link2a_binary.stl"    visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link2b"    scale="0.01" filename="link2b_binary.stl"    visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link3"     scale="0.01" filename="link3_binary.stl"     visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link3_cap" scale="0.01" filename="link3_cap_binary.stl" visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link4a"    scale="0.01" filename="link4a_binary.stl"    visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link4b"    scale="0.01" filename="link4b_binary.stl"    visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link5"     scale="0.01" filename="link5_binary.stl"     visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link5_cap" scale="0.01" filename="link5_cap_binary.stl" visual="true" collision="false" mass="false"/>
  <mesh name="mesh_link6"     scale="0.01" filename="link6_binary.stl"     visual="true" collision="false" mass="false"/>
  <sphere name="sphere_link6" radius="0.1" visual="false" collision="true"/>
    
  <body name="body_link0" dynamical="false">
    <connector name="mesh"                rot="-90deg@x"/>
    <connector name="sock" trn="0 0.19 0" rot=" 90deg@x"/>
  </body>
  <body name="body_link1">
    <connector name="mesh1"                       rot="-90deg@x"/>
    <connector name="mesh2" trn=" 0.085 0.07   0" rot=" 90deg@y"/>
    <connector name="mesh3" trn="-0.085 0.07   0" rot="-90deg@y"/>
    <connector name="plug"  trn=" 0     0.0    0" rot=" 90deg@x"/>
    <connector name="sock"  trn=" 0     0.1135 0" rot=" 90deg@y"/>
  </body>
  <body name="body_link2">
    <connector name="mesh1" trn="-0.08 0 0"     rot="90deg@y"/>
    <connector name="mesh2" trn=" 0    0.07  0"              />
    <connector name="plug"  trn=" 0    0.0   0" rot="90deg@y"/>
    <connector name="sock"  trn=" 0    0.315 0" rot="90deg@x"/>
  </body>
  <body name="body_link3">
    <connector name="mesh1" trn="-0.061 0     0" rot=" 90deg@y"/>
    <connector name="mesh2" trn=" 0.066 0.06  0" rot=" 90deg@y"/>
    <connector name="mesh3" trn="-0.066 0.06  0" rot="-90deg@y"/>
    <connector name="plug"  trn=" 0     0     0" rot=" 90deg@x"/>
    <connector name="sock"  trn=" 0     0.135 0" rot=" 90deg@y"/>
  </body>
  <body name="body_link4">
    <connector name="mesh1" trn="-0.061 0     0" rot="90deg@y"/>
    <connector name="mesh2" trn=" 0     0.055 0"              />
    <connector name="plug"  trn=" 0     0     0" rot="90deg@y"/>
    <connector name="sock"  trn=" 0     0.235 0" rot="90deg@x"/>
  </body>
  <body name="body_link5">
    <connector name="mesh1" trn=" 0     0.04  0" rot=" 90deg@y"/>
    <connector name="mesh2" trn=" 0.056 0.04  0" rot=" 90deg@y"/>
    <connector name="mesh3" trn="-0.056 0.04  0" rot="-90deg@y"/>
    <connector name="plug"  trn=" 0     0     0" rot=" 90deg@x"/>
    <connector name="sock"  trn=" 0     0.194 0" rot=" 90deg@y"/>
  </body>
  <body name="body_link6">
    <connector name="mesh"             rot="90deg@y"/>
    <connector name="plug" trn="0 0 0" rot="90deg@y"/>
  </body>

  <attach connector="body_link0/mesh"  shape="mesh_base"      vmat="mat_body" />
  <attach connector="body_link1/mesh1" shape="mesh_link1"     vmat="mat_body" />
  <attach connector="body_link1/mesh2" shape="mesh_link1_cap" vmat="mat_cover"/>
  <attach connector="body_link1/mesh3" shape="mesh_link1_cap" vmat="mat_cover"/>
  <attach connector="body_link2/mesh1" shape="mesh_link2a"    vmat="mat_body" />
  <attach connector="body_link2/mesh2" shape="mesh_link2b"    vmat="mat_body" />
  <attach connector="body_link3/mesh1" shape="mesh_link3"     vmat="mat_body" />
  <attach connector="body_link3/mesh2" shape="mesh_link3_cap" vmat="mat_cover"/>
  <attach connector="body_link3/mesh3" shape="mesh_link3_cap" vmat="mat_cover"/>
  <attach connector="body_link4/mesh1" shape="mesh_link4a"    vmat="mat_body" />
  <attach connector="body_link4/mesh2" shape="mesh_link4b"    vmat="mat_body" />
  <attach connector="body_link5/mesh1" shape="mesh_link5"     vmat="mat_body" />
  <attach connector="body_link5/mesh2" shape="mesh_link5_cap" vmat="mat_cover"/>
  <attach connector="body_link5/mesh3" shape="mesh_link5_cap" vmat="mat_cover"/>
  <attach connector="body_link6/mesh"  shape="mesh_link6"     vmat="mat_body"/>
  <attach connector="body_link6/mesh"  shape="sphere_link6"/>
    
  <hinge name="joint0" pos="0.0deg" range="-180deg 180deg" sock="body_link0/sock" plug="body_link1/plug"/>
  <hinge name="joint1" pos="0.0deg" range="-90deg   90deg" sock="body_link1/sock" plug="body_link2/plug"/>
  <hinge name="joint2" pos="0.0deg" range="-180deg 180deg" sock="body_link2/sock" plug="body_link3/plug"/>
  <hinge name="joint3" pos="0.0deg" range="-120deg 120deg" sock="body_link3/sock" plug="body_link4/plug"/>
  <hinge name="joint4" pos="0.0deg" range="-180deg 180deg" sock="body_link4/sock" plug="body_link5/plug"/>
  <hinge name="joint5" pos="0.0deg" range="-120deg 120deg" sock="body_link5/sock" plug="body_link6/plug"/>
    
</namespace>
